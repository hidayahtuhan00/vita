#include <iostream>
#include <conio.h>

using namespace std;

int main() {
	
	int A[4][6];
	
	for (int b=0;b<4;b++){
		for (int k=0;k<6;k++){
			  cout<<"A["<<b<<"]["<<k<<"] = ";
			  cin>>A[b][k];
		}
		cout << endl;
	}
	
	for (int b=0;b<4;b++){
		for (int k=0;k<6;k++){
			 cout<<A[b][k]<<" ";
		}
		cout << endl;
	}
	getch();
}
